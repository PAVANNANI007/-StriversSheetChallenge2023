public class Solution 
{
	public static String reverseString(String str) 
	{
		//Write your code here
		
        String[] s = str.split(" ");
		
        int n = s.length;
		if(n<=1){
			return str;
		}
		String ans = s[n-1];
        for(int i = n-2;i>=0;i--){
           ans = ans +" "+ s[i];
		   
        }
        return ans;
	}
}
